[
  {
    "name": "SMTP",
    "template": [
      "EHLO <<domain:string>>\r\n",
      "MAIL FROM: <<sender:string>>\r\n",
      "RCPT TO: <<recipient:string>>\r\n",
      "DATA\r\n",
      "<<message:string>>\r\n.\r\n",
      "QUIT\r\n"
    ],
    "regex": "EHLO\\ (.*?)\\\r\\\nMAIL\\ FROM:\\ (.*?)\\\r\\\nRCPT\\ TO:\\ (.*?)\\\r\\\nDATA\\\r\\\n(.*?)\\\r\\\n\\.\\\r\\\nQUIT\\\r\\\n",
    "placeholders": [
      {
        "name": "domain",
        "type": "string"
      },
      {
        "name": "sender",
        "type": "string"
      },
      {
        "name": "recipient",
        "type": "string"
      },
      {
        "name": "message",
        "type": "string"
      }
    ]
  },
  {
    "name": "HELO",
